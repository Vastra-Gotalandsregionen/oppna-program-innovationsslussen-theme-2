<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="innovationsslussen-2-theme" name="Innovationsslussen Theme 2">

		<template-extension>ftl</template-extension>

		<settings>
			<setting key="portlet-setup-show-borders-default" configurable="true" type="select" value="false" options="true,false"  />
			<setting key="show-breadcrumbs" configurable="true" type="select" value="false" options="true,false" />
			<setting key="show-hero" configurable="true" type="select" value="false" options="true,false" />
		</settings>

	</theme>
</look-and-feel>
